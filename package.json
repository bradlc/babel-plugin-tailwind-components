{
  "name": "babel-plugin-tailwind-components",
  "version": "0.2.2",
  "description": "Use Tailwind with any CSS-in-JS library",
  "main": "dist/index.js",
  "scripts": {
    "babel":
      "babel --plugins=transform-object-rest-spread --no-babelrc -d build src",
    "build":
      "npm run -s babel && microbundle -i build/index.js -f cjs && microbundle -i build/macro.js -f cjs -o ./macro.js",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bradlc/babel-plugin-tailwind-components.git"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "css-in-js",
    "babel-plugin",
    "babel-plugin-macros"
  ],
  "files": ["dist", "macro.js"],
  "author": "Brad Cornes <bradlc41@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bradlc/babel-plugin-tailwind-components/issues"
  },
  "homepage":
    "https://github.com/bradlc/babel-plugin-tailwind-components#readme",
  "dependencies": {
    "babel-plugin-macros": "^2.1.0",
    "babylon": "^6.18.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "microbundle": "^0.4.3"
  }
}
